<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.autumn.fileUpload.mapper.FilesMapper">

    <resultMap id="BaseResultMap" type="com.autumn.fileUpload.entity.Files">
        <result property="id" column="id" jdbcType="VARCHAR"/>
        <result property="fileNameBefore" column="file_name_before" jdbcType="VARCHAR"/>
        <result property="fileNameAfter" column="file_name_after" jdbcType="VARCHAR"/>
